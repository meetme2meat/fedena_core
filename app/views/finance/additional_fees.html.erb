

<div id="content-header">
<%= image_tag("/images/show_finance.png") %>
   <h1><%= t('fees_text') %></h1>
   <h3><%= t('additional_fees') %></h3>
  <div id="app-back-button">
   <%= link_to_function image_tag("/images/buttons/back.png",:border => 0), "history.back()" %>
  </div>
</div>
<div id="page-yield">
  <% unless flash[:notice].nil? %><p class="flash-msg"><%= flash[:notice] %></p><% end %>
  <div class="box">
  <div class="link-box">
    <div class="link-heading"><%= link_to "#{t('create_text')}", :action => "additional_fees_create_form" %></div>
    <div class="link-descr"><%= t('create_text') %> <%= t('additional') %> <%= t('fees_text') %> </div>
  </div>

  <div class="link-box">
    <div class="link-heading"><%= link_to "#{t('view')}", :action => "additional_fees_list" %></div>
    <div class="link-descr"><%= t('view') %> <%= t('additional') %> <%= t('fees_text') %></div>
  </div>

</div>
</div>