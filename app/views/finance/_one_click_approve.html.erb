


<%unless @dates.empty? %>
  <div id="submit-button" class ="approve">
      <%= link_to t('approve'), {:controller=>"finance", :action=>"one_click_approve_submit",:date => @salary_date } %>
  </div>
<% else%>
  <div id="submit-button" class ="approved">
    <%= t('already_approved') %>
  </div>
<% end %>



